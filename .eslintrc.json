{
  "extends": "next/core-web-vitals",
  "rules": {
    "@next/next/no-img-element": "off",
    "jsx-a11y/alt-text": "off",
    "react/display-name": "off",
    "no-undef-init": 1,
    "no-trailing-spaces": 0,
    "no-unused-vars": 0,
    "no-param-reassign": 0,
    "space-before-function-paren": [
      "error",
      {
        "anonymous": "always",
        "named": "always",
      }
    ]
  }
}
